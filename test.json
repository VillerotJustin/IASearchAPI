{
    "filter_config": {
        "project_name": "HUMANE",
        "ressource": "ns0__record",
        "filters": {
            "ns0__setSpec": ["PHY", "ns0__has_setSpec"]
        },
        "nb_different_criteria_for_each_data":30,
        "threshold_percentage_max_filter":0.1,
        "WITH": {
            "ressource.ns0__identifier": "ID",
            "ressource.ns0__title_string": "title",
            "ressource.ns0__description_string": "description",
            "collect(taxon.ns0__entry_string)": "phrases"
        },
        "RETURN": [
            "ID",
            "title",
            "description",
            "reduce(s='', phrase IN phrases | s + phrase) AS concatenated_taxon"
        ],
        "taxon": {
            "ns0__has_taxon": "ns0__taxon"
        }
    },
    "recomandation_config": {
        "user_request": "la trajectoire de la courbe est parabolique",
        "recomandation_method": "TF-IDF+Word2Vec",
        "similarity_method": "cosine similarity",
        "number_of_recomandation": 5
    }


}